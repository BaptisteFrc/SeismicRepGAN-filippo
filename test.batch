## For ntasks and cpus: total requested cpus (ntasks * cpus-per-task) must be in [1: 4 >
#SBATCH --ntasks=1
#SBATCH --cpus-per-task=4


python RepGAN_drive.py --nX 2000 --Xsize 480 --cuda  --epochs 500 --latentSdim 2 --latentNdim 256 --nXRepX 3 --nRepXRep 1 --nCritic 1 --nGenerator 3 --nSlayers 1 --nNlayers 1 --nClayers 1 --DxLR 0.00002 --DsLR 0.00011 --DnLR 0.00011 --DcLR 0.00011 --FxLR 0.00011 --GzLR 0.00011 --store_dir 'dataset' --checkpoint_dir 'dataset/checkpoint' --results_dir 'dataset/results/ActtanhLR0011500' --trainVeval 'TRAIN'
